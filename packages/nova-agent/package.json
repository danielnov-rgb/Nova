{
  "name": "@nova/agent",
  "version": "0.1.0",
  "description": "CLI tool for automated feature discovery and Nova plugin instrumentation",
  "type": "module",
  "bin": {
    "nova-agent": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "conf": "^13.0.0",
    "glob": "^11.0.0",
    "ora": "^8.0.1",
    "simple-git": "^3.25.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nova",
    "analytics",
    "feature-discovery",
    "cli",
    "instrumentation"
  ],
  "license": "MIT"
}
